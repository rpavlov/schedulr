<span id="provider-my-listings">
    <div class="row">
        <div class="col-md-6">
            <h3>My Open Listings</h3>
        </div>
        <div class="col-md-6">
            <%= link_to "New Listing", new_provider_listing_path, class:"btn btn-success"%>
        </div>
    </div>
    <br/>
    <div id="listings_partial">
        <div class="row">
            <div class="col-md-4"><strong>Provider name</strong></div>
            <div class="col-md-4"><strong>Start Date</strong></div>
            <div class="col-md-4"><strong>End Date</strong></div>
        </div>
        <hr/>
        <% @available_listings.each do |listing|  %>
            <div class="row">
                <div class="col-md-3"><%=listing.provider.name%></div>
                <div class="col-md-4"><%=listing.start_at_label %></div>
                <div class="col-md-4"><%=listing.end_at_label %></div>
                <div class="col-md-1"><%= link_to 'X', provider_listing_path(listing), method: :delete, data: { confirm: 'Are you sure?' }, class:"btn btn-danger" %></div>
            </div>
        <% end %>
    </div>
</span>
